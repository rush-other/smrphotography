<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html>
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1">
        <link href="/stylesheets/default.css?<%=css_time_stamp%>" type="text/css" rel="stylesheet"></link>
        <%= javascript_include_tag :defaults %>
		<title>
			<%= page_title %>
    	</title>
	</head>
	<body>
        <table class="main_frame">
            <tr>
                <td class="main_nav" colspan="2">
                    <table class="nav_links">
                        <tr>
                            <td>
                                <% if selected('portraits') %>
                                    <img src="/images/portraitsroll.gif" />
                                <% else %>
                                    <a href="/portraits.html">
                                        <img 
                                            src="/images/portraits.gif" 
                                            onmouseover="javascript:img_hover(this, 'portraitsroll.gif', false);" 
                                            onmouseout="javascript:img_hover(this, 'portraits.gif', true);"
                                            onclick="javascript:set_page('portraits.gif');" 
                                        />
                                    </a>
                                <% end %>
                            </td>
                            <td>
                                <% if selected('pricing') %>
                                    <img src="/images/pricingroll.gif" />
                                <% else %>
                                    <a href="/pricing.html">
                                        <img 
                                            src="/images/pricing.gif" 
                                            onmouseover="javascript:img_hover(this, 'pricingroll.gif', false);" 
                                            onmouseout="javascript:img_hover(this, 'pricing.gif', true);"
                                            onclick="javascript:set_page('pricing.gif');" 
                                        />
                                    </a>
                                <% end %>
                            </td>
                            <td>
                                <% if selected('about') %>
                                    <img src="/images/aboutroll.gif" />
                                <% else %>
                                    <a href="/about.html">
                                        <img 
                                            src="/images/about.gif" 
                                            onmouseover="javascript:img_hover(this, 'aboutroll.gif', false);" 
                                            onmouseout="javascript:img_hover(this, 'about.gif', true);"
                                            onclick="javascript:set_page('about.gif');" 
                                        />
                                    </a>
                                <% end %>
                            </td>
                            <td>
                                <% if selected('contact') %>
                                    <img src="/images/blogroll.gif" />
                                <% else %>
                                    <a href="/blog">
                                        <img 
                                            src="/images/blog.gif" 
                                            onmouseover="javascript:img_hover(this, 'blogroll.gif', false);" 
                                            onmouseout="javascript:img_hover(this, 'blog.gif', true);"
                                            onclick="javascript:set_page('blog.gif');" 
                                        />
                                    </a>
                                <% end %>
                            </td>
                            <td>
                                <% if selected('client_galleries') %>
                                    <% if controller.action_name == 'client_gallery' %>
                                        <a href="/client_galleries.html">
                                            <img 
                                                src="/images/clientgalleriesroll.gif" 
                                                onmouseover="javascript:img_hover(this, 'clientgalleriesroll.gif', false);" 
                                                onmouseout="javascript:img_hover(this, 'clientgalleries.gif', true);"
                                                onclick="javascript:set_page('clientgalleries.gif');" 
                                            />
                                        </a>
                                    <% else %>
                                        <img src="/images/clientgalleriesroll.gif" />
                                    <% end %>
                                <% else %>
                                    <a href="/client_galleries.html">
                                        <img 
                                            src="/images/clientgalleries.gif" 
                                            onmouseover="javascript:img_hover(this, 'clientgalleriesroll.gif', false);" 
                                            onmouseout="javascript:img_hover(this, 'clientgalleries.gif', true);"
                                            onclick="javascript:set_page('clientgalleries.gif');" 
                                        />
                                    </a>
                                <% end %>
                            </td>
                            <td>
                                <% if selected('current') %>
                                    <img src="/images/currentprojectsroll.gif" />
                                <% else %>
                                    <a href="/current.html">
                                        <img 
                                            src="/images/currentprojects.gif" 
                                            onmouseover="javascript:img_hover(this, 'currentprojectsroll.gif', false);" 
                                            onmouseout="javascript:img_hover(this, 'currentprojects.gif', true);"
                                            onclick="javascript:set_page('currentprojects.gif');" 
                                        />
                                    </a>
                                <% end %>
                            </td>
                            <td>
                                <img src="/images/tabbarside.gif"/>
                            </td>
                        </tr>
                    </table>
                </td>
            </tr>
            <tr>
                <td class="body">
                    <% if not flash[:messages].blank? %>
                        <div class="messages">
                            <ul>
                                <% flash[:messages].each do |msg| %>
                                    <li><%= msg %></li>
                                <% end %>
                            </ul>
                        </div>
                    <% end %>
                    <%= yield %>
                </td>
                <td class="sidebar" rowspan="2">
                    <div class="logo_div">
                        <a href="<%= link_to_home %>">
                            <img src="/images/mainlogo.gif"/>
                        </a>
                    </div>
                    <div class="sidebar_div">
                        <%= render_sidebar_menu_if_applicable %>
                    </div>
                    <div class="subscribe_div">
                        <div id="subscribe_message" name="subscribe_message"></div>
                        <% if controller.action_name == "current" %>
                            &nbsp;Subscribe to email updates for
                            <br/>
                            &nbsp;Portrait of a Vegan:
                        <% else %>
                            &nbsp;Subscribe to the SMR Photography
                            <br/>
                            &nbsp;Newsletter for news and special offers:
                        <% end %>
                        <br/>
                        <table>
                            <tr>
                                <td>
                                    <input type="text" id="subscribe" name="subscribe" class="drop_shadow" onkeydown="javascript:enter_subscription(event, '<%=controller.action_name%>', 'subscribe_message');"/>
                                </td>
                                <td>
                                    <a href="#" onclick="javascript:submit_subscription('<%=controller.action_name%>', 'subscribe_message');">
                                        <img src="/images/boxarrow.gif"/>
                                    </a>
                                </td>
                           </tr>
                           <tr>
                                <td>
                            <div class="contact">
                            <br/>
                            <b>Contact:</b><br/>
                            <a href="mailto:shannon@shannonmrush.com">shannon@shannonmrush.com</a><br/>
                            310.401.4333
                            </div>
                            </td>
                            </tr>
                            </table>
                    </div>
                </td>
            </tr>
        </table>
		<script type="text/javascript">
		var gaJsHost = (("https:" == document.location.protocol) ? "https://ssl." : "http://www.");
		document.write(unescape("%3Cscript src='" + gaJsHost + "google-analytics.com/ga.js' type='text/javascript'%3E%3C/script%3E"));
		</script>
		<script type="text/javascript">
		try {
		var pageTracker = _gat._getTracker("UA-12593174-3");
		pageTracker._trackPageview();
		} catch(err) {}</script>
    </body>
</html>